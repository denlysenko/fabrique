<%if(locals.results.length){%>
	<div class="table-responsive">
    <table class="table edit">
      <thead>
        <tr>
          <th>Code</th>
          <th>Category</th>
          <th>Title</th>
          <th>Price</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
	<% results.forEach(function(result){%>
      <tr>
        <td><%= result.productCode %></td>
        <td><%= result.category %></td>
        <td><%= result.title %></td>
        <td><%= result.price %></td>
        <td>
          <% if(page === 'edit'){%>
            <a href="/api/products/edit/<%= result.productCode %>">Show Product</a>
          <%} else if(page === 'slider'){%>
            <a href="/api/slider/<%= result.productCode %>">Show Product</a>
          <%} else {%>
            <a href="/api/sale/<%= result.productCode %>">Show Product</a>
          <%}%>    
        </td>
      </tr>
	<%})%>
	    </tbody>
	  </table>
	</div>
  <% removeResults() %>
<%}%>	